snippet forr
abbr    for range
options head
  for ${1:v} := range ${2:#:iterator} {
    ${0:TARGET}
  }

snippet pn
abbr    fmt.Println
options word
  fmt.Println(${0:TARGET})

snippet pf
abbr    fmt.Printf
options word
  fmt.Printf(${0:TARGET})

snippet p#
abbr    fmt.Printf # adverb
options word
  fmt.Printf("%#v\n", ${0:TARGET})

snippet     gof
abbr        go func() { ... }()
options     head
  go func(${2}) ${3:int }{
    ${0:TARGET:return }
  }(${1:#:args})

snippet     funm
abbr        func (receiver) ...() { ... }
options     head
  func (${1:#:receiver}) ${2:mname}(${3}) ${4:int }{
    ${0:TARGET:return }
  }
